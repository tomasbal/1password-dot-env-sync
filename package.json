    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@types/diff": "^5.2.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
